{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint; npm run lint:style",
    "lint:fix": "npx eslint src/ --fix; npm run lint:style:fix",
    "lint:style": "npx stylelint --custom-formatter './stylelint-formatter.js' 'src/**/*.scss'",
    "lint:style:fix": "npx stylelint --custom-formatter './stylelint-formatter.js' 'src/**/*.scss' --fix",
    "e2e": "ng e2e",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "beautify-html": "npx html-beautify --replace -s 2 -n -w 140 -A force -m 2 'src/**/*.component.html'",
    "prettier": "npx prettier --config ./.prettierrc.json --write src",
    "format-interpolation": "npx replace-in-file --configFile=./template-format-config.js"
  },
  "lint-staged": {
    "**/*{.ts,.scss,.json}": [
      "prettier --write --ignore-unknown"
    ],
    "**/*.html": [
      "npx html-beautify --replace -s 2 -n -w 140 -A force -m 2",
      "npm run format-interpolation"
    ],
    "**/*.scss": [
      "npm run lint:style:fix"
    ]
  },
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/fire": "^6.1.5",
    "@angular/forms": "~12.2.0",
    "@angular/material": "^12.2.0",
    "@angular/platform-browser": "^12.2.0",
    "@angular/platform-browser-dynamic": "^12.2.0",
    "@angular/router": "~12.2.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "firebase": "^8.9.1",
    "moment": "^2.29.1",
    "rxfire": "^4.0.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.0.0",
    "virmator": "^1.1.8",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.0",
    "@angular-eslint/builder": "1.0.0",
    "@angular-eslint/eslint-plugin": "1.0.0",
    "@angular-eslint/eslint-plugin-template": "1.0.0",
    "@angular-eslint/schematics": "^12.3.1",
    "@angular-eslint/template-parser": "1.0.0",
    "@angular/cli": "~12.2.0",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.20.19",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "^6.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-decorator-position": "^2.2.29",
    "eslint-plugin-deprecation": "^1.2.1",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsdoc": "^36.0.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-rxjs": "^3.3.5",
    "husky": "^7.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "js-beautify": "^1.14.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "protractor": "~7.0.0",
    "replace-in-file": "^6.2.0",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-plugin-color": "^1.1.1",
    "stylelint-scss": "^3.20.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
